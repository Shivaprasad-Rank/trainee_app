<div class="table-responsive">
<h2>List Of Colleges</h2>
<table class="table">
	<tr>
		<td>
			College Name:
		</td>
		<td>
			<%= @college.colname %>
		</td>
	</tr>
	<tr>
		<td>
			College Address:
		</td>
		<td>
			<%= @college.coladd %>
		</td>
	</tr>
	<tr>
		<td>
			College Photo:
		</td>
		<td>
			<%= image_tag @college.image %>
		</td>
	</tr>
	<tr>
		
		<td>
			<%= link_to "Home Page", colleges_path %>
		</td>
	</tr>
</table>
</div>